CREATE TABLE if not exists Funcionario (
  id INTEGER primary key AUTOINCREMENT,
  cargo VARCHAR(100) NOT NULL,
  nome VARCHAR(100) NOT NULL,
  cpf VARCHAR(14) NOT NULL UNIQUE,
  endereco TEXT,
  telefone VARCHAR(15),
  idade INTEGER
  );
  
