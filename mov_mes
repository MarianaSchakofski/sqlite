CREATE TABLE if not exists movimento_mes (
  id_movimento INTEGER primary key AUTOINCREMENT,
  cliente varchar(50) not NULL,
  treinos_feitos varchar(10) NOT NULL,
  faltas varchar(10) NOT NULL 
  );
