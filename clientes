CREATE TABLE if not exists clientes (
  id INTEGER primary key AUTOINCREMENT,
  nome VARCHAR(100) NOT NULL,
  idade INTEGER
  telefone VARCHAR(15),
  telefone_emergencia VARCHAR(15),
  endereco TEXT,
  email VARCHAR(100),
  cpf VARCHAR(14) NOT NULL UNIQUE,
  );
  
